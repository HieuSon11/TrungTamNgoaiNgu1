@model MyTTNN.TrungTamNgoaiNgu.ChucVu

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/NhanVien/Views/Shared/_NhanVienLayout.cshtml";
}


<div class="breadcrumbs">
    <div class="col-sm-4">
        <div class="page-header float-left">
            <div class="page-title">
                <h1>Chức Vụ</h1>
            </div>
        </div>
    </div>
    <div class="col-sm-8">
        <div class="page-header float-right">
            <div class="page-title">
                <ol class="breadcrumb text-right">
                    <li><a href="#">Chức Vụ</a></li>
                    <li class="active">Chi tiết</li>
                </ol>
            </div>
        </div>
    </div>
</div>

<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">

            <div class="col-md-12">
                <div class="card">
                    <div class="card-header row">
                        <div class="col-md-6">
                            <h2><strong class="card-title">Chi tiết</strong></h2>
                        </div>
                        <div class="col-md-6 text-right">
                            <a href="@Url.Action("Index", "ChucVus")" class="btn btn-default">Trở về</a>
                        </div>
                    </div>

                    <div class="card-body">
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.TenCv)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.TenCv)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.NguoiTao)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.NguoiTao)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.NguoiCapNhat)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.NguoiCapNhat)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.ThoiGianTao)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.ThoiGianTao)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.ThoiGianCapNhat)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.ThoiGianCapNhat)
                            </dd>

                        </dl>
                    </div>
                </div>
            </div>


        </div>

    </div><!-- .content -->

</div><!-- .content -->
